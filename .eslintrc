{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "jsx": true,
      "modules": true
    }
  },
  "plugins": ["react", "react-hooks", "prettier"],
  "globals": {
    "$": true,
    "Handlers": true,
    "LocalTime": true,
    "Routes": true,
    "Turbolinks": true,
    "__dirname": true,
    "gon": true,
    "google": true,
    "ga": true,
    "fbq": true,
    "harvest": true,
    "module": true,
    "require": true
  },
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "react/jsx-closing-bracket-location": 0,
    "react/prop-types": 0,
    "react/display-name": 0,
    "quote-props": ["error", "consistent-as-needed"],
    "no-debugger": 0,
    "no-undef": 0,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "no-console": 0,
    "linebreak-style": ["error", "unix"],
    "prettier/prettier": [
      "error",
      {
        "arrowParens": "avoid",
        "bracketSpacing": false,
        "jsxBracketSameLine": true,
        "printWidth": 80,
        "singleQuote": true,
        "trailingComma": "all",
        "quoteProps": "consistent",
        "parser": "flow"
      },
      { "usePrettierrc": false }
    ]
  }
}
